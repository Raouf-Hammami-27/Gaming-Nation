@model GUI.Models.EvenementModels

@{
    ViewBag.Title = "Delete";

}
<div class="main-container">
    <!-- Container fluid Starts -->
    <div class="container-fluid">
        <!-- Spacer starts -->
        <div class="spacer">
            <!-- Row Starts -->
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="blog">
                        <div class="blog-header">
                            <h5 class="blog-title">Are you sure you want to delete this tournament?</h5>
                        </div>
                        <div class="blog-body">
                            <form class="form-inline" role="form">

                                <div class="blog-body">
                                    <div class="demo-btn-group center-align-text">
                                        <button type="submit" class="fa fa-save btn btn-default"> Delete</button>
                                        <button class="fa fa-mail-reply btn btn-success" onclick="location.href='@Url.Action("Index", "Evenement")';return false;"> Cancel</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>